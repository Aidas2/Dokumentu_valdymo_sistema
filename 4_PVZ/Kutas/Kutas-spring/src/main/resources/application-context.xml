<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xmlns="http://www.springframework.org/schema/beans"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
       http://www.springframework.org/schema/context
       http://www.springframework.org/schema/context/spring-context-3.0.xsd">
        <context:component-scan base-package="lt.sventes"/>
        <context:annotation-config/>

        <bean class="it.akademija.Product">
                <constructor-arg name="id" value="1"/>
                <constructor-arg name="title" value="Iphone"/>
                <constructor-arg name="description" value="very good phone"/>
                <constructor-arg name="imageUrl" value="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcSG5e6Z5vwVufloDYsfAbppZ41qo9Ix6a-22gn2N92d4o4uk-sC"/>
                <constructor-arg name="price" value="500.00"/>

        </bean>
</beans>